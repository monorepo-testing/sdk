{
    "name": "netflex/sdk",
    "require": {
        "php": "^7.4",
        "illuminate/support": "^8.72"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Netflex\\": "src/Netflex/",
            "Netflex\\Contracts\\": "src/Netflex/Contracts/",
            "Netflex\\Test\\": "src/Netflex/Test/"
        }
    },
    "replace": {
        "netflex/contracts": "self.version",
        "netflex/test": "self.version"
    }
}
